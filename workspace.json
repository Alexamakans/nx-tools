{
  "version": 2,
  "projects": {
    "ci-context": {
      "root": "packages/ci-context",
      "sourceRoot": "packages/ci-context/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/ci-context",
            "tsConfig": "packages/ci-context/tsconfig.lib.json",
            "packageJson": "packages/ci-context/package.json",
            "main": "packages/ci-context/src/index.ts",
            "assets": ["packages/ci-context/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/ci-context/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/ci-context"],
          "options": {
            "jestConfig": "packages/ci-context/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "core": {
      "root": "packages/core",
      "sourceRoot": "packages/core/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/core",
            "tsConfig": "packages/core/tsconfig.lib.json",
            "packageJson": "packages/core/package.json",
            "main": "packages/core/src/index.ts",
            "assets": ["packages/core/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/core/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/core"],
          "options": {
            "jestConfig": "packages/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "docker-meta": {
      "root": "packages/docker-meta",
      "sourceRoot": "packages/docker-meta/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/docker-meta",
            "tsConfig": "packages/docker-meta/tsconfig.lib.json",
            "packageJson": "packages/docker-meta/package.json",
            "main": "packages/docker-meta/src/index.ts",
            "assets": ["packages/docker-meta/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/docker-meta/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/docker-meta"],
          "options": {
            "jestConfig": "packages/docker-meta/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nx-docker": {
      "root": "packages/nx-docker",
      "sourceRoot": "packages/nx-docker/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/nx-docker/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/nx-docker"],
          "options": {
            "jestConfig": "packages/nx-docker/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/nx-docker",
            "tsConfig": "packages/nx-docker/tsconfig.lib.json",
            "packageJson": "packages/nx-docker/package.json",
            "main": "packages/nx-docker/src/index.ts",
            "assets": [
              "packages/nx-docker/*.md",
              {
                "input": "./packages/nx-docker/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/nx-docker",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./packages/nx-docker",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "nx-prisma": {
      "root": "packages/nx-prisma",
      "sourceRoot": "packages/nx-prisma/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/nx-prisma/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/nx-prisma"],
          "options": {
            "jestConfig": "packages/nx-prisma/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/nx-prisma",
            "tsConfig": "packages/nx-prisma/tsconfig.lib.json",
            "packageJson": "packages/nx-prisma/package.json",
            "main": "packages/nx-prisma/src/index.ts",
            "assets": [
              "packages/nx-prisma/*.md",
              {
                "input": "./packages/nx-prisma/src",
                "glob": "**/!(*.ts)",
                "output": "./src"
              },
              {
                "input": "./packages/nx-prisma/src",
                "glob": "**/*.d.ts",
                "output": "./src"
              },
              {
                "input": "./packages/nx-prisma",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./packages/nx-prisma",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
