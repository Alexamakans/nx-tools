{
  "name": "@nx-tools/nx-set-shas",
  "version": "1.0.0-alpha.6",
  "type": "commonjs",
  "bin": {
    "nx-set-shas": "./bin/run"
  },
  "files": [
    "/bin",
    "/src",
    "/oclif.manifest.json"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@oclif/core": "1.7.0",
    "@oclif/plugin-help": "5.1.12",
    "@swc/helpers": "^0.3.17",
    "undici": "5.0.0"
  },
  "oclif": {
    "bin": "nx-set-shas",
    "dirname": "nx-set-shas",
    "commands": "./src/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Calculate the base and head SHAs required for `nx affected` commands in CI"
      }
    }
  },
  "scripts": {
    "prepack": "npx oclif manifest && npx oclif readme"
  }
}
